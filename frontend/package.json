{
  "name": "frontend",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 500",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:count": "tsc --noEmit --skipLibCheck 2>&1 | grep -E \"error TS[0-9]+\" | wc -l || echo \"0\"",
    "typecheck:errors": "tsc --noEmit --skipLibCheck 2>&1 | grep -E \"error TS[0-9]+\" || echo \"No TypeScript errors found\"",
    "typecheck:prod": "tsc --noEmit --project tsconfig.json",
    "typecheck:test": "tsc --noEmit --project tsconfig.test.json",
    "typecheck:all": "npm run typecheck:prod && npm run typecheck:test",
    "cleanup:test-dirs": "./scripts/cleanup-test-dirs.sh"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.12.0",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.3",
    "recharts": "^2.10.0",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^25.0.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.11.1",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "fast-check": "^4.5.3",
    "globals": "^17.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.32",
    "typescript": "^5.2.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
