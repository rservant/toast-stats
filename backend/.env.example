# Server Configuration
PORT=5000
NODE_ENV=development

# Toastmasters Dashboard API
TOASTMASTERS_DASHBOARD_URL=https://dashboard.toastmasters.org

# JWT Configuration
JWT_SECRET=your-secret-key-change-in-production
