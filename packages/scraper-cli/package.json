{
  "name": "@toastmasters/scraper-cli",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "description": "Standalone CLI tool for scraping Toastmasters dashboard data",
  "bin": {
    "scraper-cli": "./bin/scraper-cli.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "playwright": "^1.58.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.39.0",
    "fast-check": "^4.5.2",
    "globals": "^17.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
